{
    "manifest_version": 2,
    "name": "Virtual Room",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": [
      "activeTab",
      "https://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon128.png"
      }
    },
    "icons": {
      "128": "icon128.png"
    }
  }